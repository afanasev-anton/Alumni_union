{% extends 'base.html.twig' %}

{% block title %}Alumni Association | Members{% endblock %}

{% block body %}

	<h1>List of Alumni</h1>

	{% for user in users %}
		<p>{{user.username}} | <a href="/alumni/profile={{user.id}}">{{user.email}}</a> | 
			{% for role in user.roles %}
				{{role}}
			{% endfor %}
		</p>
	{% endfor %}

{% endblock %}